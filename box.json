{
    "chmod": "0755",
    "main": "bin/srca",
    "output": "release/srca.phar",
    "files": [
        "README.md"
    ],
    "directories": [
        "src/",
        "config/",
        "vendor/"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
                "Tests",
                "tests",
                "phpunit",
                "var-dumper"
            ],
            "in": "vendor"
        }
    ],
    "stub": true
}
