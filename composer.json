{
    "name": "gckabir/srca",
    "description": "Source Code Analyzer tool",
    "require": {
        "php": ">=5.5.9",
        "symfony/console": "^2.7",
        "illuminate/filesystem": "^5.1",
        "illuminate/container": "^5.1",
        "nikic/php-parser": "^1.3"
    },
    "require-dev": {
        "symfony/var-dumper": "^2.7",
        "phpunit/phpunit": "4.5.*"
    },
    "autoload": {
        "psr-4": {
            "Analyzer\\": "src/"
        },
        "files": [
            "src/Misc/helpers.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "composer dump-autoload --optimize"
        ],
        "post-update-cmd": [
            "composer dump-autoload --optimize"
        ]
    },
    "bin": [
        "bin/srca"
    ]
}
